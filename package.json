{
  "name": "ask-cli",
  "version": "2.22.4",
  "description": "Alexa Skills Kit (ASK) Command Line Interfaces",
  "bin": {
    "ask": "dist/bin/ask.js"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "license": "Apache 2.0",
  "repository": "alexa/ask-cli",
  "author": {
    "name": "RonWang",
    "email": "nongwang@amazon.com"
  },
  "keywords": [
    "alexa",
    "skill",
    "cli",
    "package",
    "cloudformation",
    "serverless",
    "deploy"
  ],
  "scripts": {
    "build:bin": "babel -d dist/bin --extensions \".js,.ts\" bin",
    "build:lib": "babel -d dist/lib --extensions \".js,.ts\" lib",
    "build": "npm-run-all build:lib build:bin",
    "clean:ask": "node scripts/ask-clean-up.js",
    "clean:aws": "node scripts/aws-clean-up.js",
    "clean": "npm-run-all clean:aws clean:ask",
    "nyc:test": "nyc --cache npm test",
    "nyc:report": "nyc report --reporter=html",
    "nyc:open": "open coverage/index.html",
    "test": "node ./node_modules/mocha/bin/_mocha -t 10000 -u exports -R spec test/unit/run-test.js",
    "test:watch": "node ./node_modules/mocha/bin/_mocha test/unit/run-test.js -w",
    "test:report": "npm-run-all nyc:test nyc:report",
    "test:browser": "npm-run-all test:report nyc:open",
    "integration-test": "mocha -t 180000 test/integration/run-test.js",
    "functional-test": "mocha -t 600000 --retries 2 test/functional/run-test.js",
    "functional-test:clean-up": "npm run clean",
    "lint": "eslint lib/builtins lib/clients lib/commands lib/controllers lib/model lib/view",
    "lint:bin": "eslint \"bin/**/*.{js,ts}\"",
    "prepublishOnly": "npm run build",
    "pre-release": "standard-version",
    "prism": "prism",
    "postinstall": "node postinstall.js"
  },
  "dependencies": {
    "adm-zip": "^0.5.5",
    "archiver": "^5.3.0",
    "ask-smapi-model": "^1.15.2",
    "ask-smapi-sdk": "^1.2.0",
    "async": "^3.2.0",
    "aws-profile-handler": "2.0.3",
    "aws-sdk": "^2.288.0",
    "axios": "^0.21.1",
    "bunyan": "^1.8.12",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "date-fns": "^2.7.0",
    "folder-hash": "^4.0.1",
    "inquirer": "^8.0.0",
    "js-yaml": "^4.1.0",
    "jsonfile": "^6.1.0",
    "listr": "^0.14.3",
    "module-alias": "^2.1.0",
    "mustache": "^4.0.1",
    "nodemon": "^2.0.4",
    "open": "^8.0.9",
    "ora": "^5.4.0",
    "portscanner": "^2.1.1",
    "pretty-bytes": "^5.1.0",
    "ramda": "^0.27.0",
    "request": "^2.79.0",
    "rxjs": "^7.0.1",
    "semver": "^7.3.5",
    "simple-git": "^2.39.0",
    "tmp": "^0.2.1",
    "uuid": "^8.3.2",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@stoplight/prism-cli": "^4.2.2",
    "@types/adm-zip": "^0.4.34",
    "@types/archiver": "^5.1.0",
    "@types/async": "^3.2.6",
    "@types/bunyan": "^1.8.6",
    "@types/folder-hash": "^4.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.1",
    "@types/js-yaml": "^4.0.1",
    "@types/listr": "^0.14.3",
    "@types/node": "^15.3.0",
    "@types/portscanner": "^2.1.0",
    "@types/ramda": "^0.27.40",
    "@types/request": "^2.48.5",
    "@types/semver": "^7.3.6",
    "@types/tmp": "^0.2.0",
    "@types/uuid": "^8.3.0",
    "@types/valid-url": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.3.4",
    "chai-json-schema": "^1.5.1",
    "chai-uuid": "^1.0.6",
    "coveralls": "^3.0.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "fs-extra": "^10.0.0",
    "husky": "^6.0.0",
    "mocha": "^8.4.0",
    "mocha.parallel": "^0.15.6",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^10.0.0",
    "standard-version": "^9.3.0",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test:report"
    }
  },
  "nyc": {
    "check-coverage": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "include": [
      "lib/utils/string-utils.js",
      "lib/utils/url-utils.js",
      "lib/utils/zip-utils.js",
      "lib/utils/hash-utils.js",
      "lib/utils/retry-utility.js",
      "lib/builtins/*",
      "lib/clients/*",
      "lib/model/*",
      "lib/view/*",
      "lib/controllers/*",
      "lib/commands/*"
    ],
    "exclude": [
      "lib/clients/aws-client/aws-util.js"
    ]
  },
  "_moduleAliases": {
    "@root": "../",
    "@src": "./lib",
    "@test": "./test"
  }
}
